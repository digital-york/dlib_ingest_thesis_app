<p id="notice"><%= notice %></p>

<h1>Listing Theses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Title</th>
      <th>Date</th>
      <th>Abstract</th>
      <th>Degreetype</th>
      <th>Supervisor</th>
      <th>Department</th>
      <th>Subjectkeyword</th>
      <th>Rightsholder</th>
      <th>Licence</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @theses.each do |thesis| %>
      <tr>
        <td><%= thesis.name %></td>
        <td><%= thesis.title %></td>
        <td><%= thesis.date %></td>
        <td><%= thesis.abstract %></td>
        <td><%= thesis.degreetype %></td>
        <td><%= thesis.supervisor %></td>
        <td><%= thesis.department %></td>
        <td><%= thesis.subjectkeyword %></td>
        <td><%= thesis.rightsholder %></td>
        <td><%= thesis.licence %></td>
        <td><%= link_to 'Show', thesis %></td>
        <td><%= link_to 'Edit', edit_thesis_path(thesis) %></td>
        <td><%= link_to 'Destroy', thesis, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Thesis', new_thesis_path %>
